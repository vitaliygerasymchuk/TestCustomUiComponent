<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomVerticalSlider">
        <attr name="data_type" format="enum">
            <enum name="temperature" value="0" />
            <enum name="humidity" value="1" />
            <enum name="windSpeed" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SetValueView">
        <attr name="set_data_type" format="enum">
            <enum name="temperature" value="0" />
            <enum name="humidity" value="1" />
            <enum name="windSpeed" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SenseSectionCard">
        <attr name="sense_type" format="enum">
            <enum name="sense_reality" value="0" />
            <enum name="sense_detailed" value="1" />
            <enum name="sense_abstract" value="2" />
        </attr>
        <attr name="sense_image_resource" format="reference" />
        <attr name="sense_title" format="string" />
    </declare-styleable>

</resources>